<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="layout_border_primaryColor">
        <item name="android:background">@drawable/borde_pintado</item>
    </style>

    ---! Texto ---

    <style name="texto_item_headboard">
        <item name="android:background">@drawable/item_headboard_background</item>
        <item name="android:textColor">@color/Factura3</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textAlignment">center</item>
        <item name="backgroundTint">@color/primary</item>
        <item name="android:paddingStart">2dp</item>
    </style>
    <style name="texto_item">
        <item name="android:background">@drawable/item_background</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">10sp</item>
        <item name="android:padding">2dp</item>
    </style>
    <style name="texto_resalted_1">
        <item name="android:textColor">@color/Factura3</item>
    </style>
    <style name="texto_resalted_2">
        <item name="android:textColor">@color/primareVariation</item>
    </style>
    <style name="button_factura">
        <item name="android:background">@drawable/button_factura_backgroud</item>
        <item name="android:textColor">@color/SecondaryVariationD</item>
        <item name="android:textSize">10sp</item>
    </style>
    <style name="input_text_dialog" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeColor">@color/primary</item>
        <item name="android:textColorHint">@color/hint</item>
    </style>
</resources>
